{
    "name": "indigophp/scaleway",
    "description": "Consume Scaleway API",
    "license": "MIT",
    "keywords": ["scaleway"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/im-denisenko/ivory-http-adapter.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "egeloen/http-adapter": "dev-fix-composer-json as 0.7.*@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0",
        "ciaranmcnulty/phpspec-typehintedmethods": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Scaleway\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "spec/HttpAdapterAuthBehavior.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
